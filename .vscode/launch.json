{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: ss.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ss.py",
            "args": ["-w", "-is", "--translate=jp,en", "-fps"],
            "console": "integratedTerminal"
        },
        {
            "name": "venv_r5 Python: ss.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ss.py",
            "args": ["-is", "--translate=jp,en", "-fps"],
            "console": "integratedTerminal",
            "python": "venv_r5/bin/python"
        },
        {
            "name": "webserv (old)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/webserv.py",
            "args": [""],
            "console": "integratedTerminal",
            "python": "venv_r5/bin/python"
        },
        {
            "name": "stream video",
            "type": "debugpy",
            "request": "launch",
            "module": "server.stream_http_video",
            "console": "integratedTerminal",
            "python": "venv/bin/python"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python so: ss.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ss.py",
            "args": ["-w", "-is", "--translate=jp,en", "-fps", "-so", "--method=openai"],
            "console": "integratedTerminal"
        }
    ]
}